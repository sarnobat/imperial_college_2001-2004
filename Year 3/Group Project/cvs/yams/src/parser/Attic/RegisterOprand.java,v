text
@/*
 * Created on 06-Nov-2003
 */

package parser;

/**
 * Models a register as an oprand of an instruction.
 * 
 * @@author qq01
 */
public class RegisterOprand extends Oprand {
	private String value;
	
	public RegisterOprand(String value){
		this.value = value;
	}

	/**
	 * Returns the value of the oprand.
	 * 
	 * @@return the value of the oprand.
	 */
	public String getValue(){
		return value;
	}
}
