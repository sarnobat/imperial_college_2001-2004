public class ToBeDoneRow
{
	private int address;
	private int operandToBeUpdated;
	private String labelName;
	private String instructionType;
		
	public ToBeDoneRow(int a, int op, String l, String t)
	{
		address=a; operandToBeUpdated=op; labelName=l; instructionType=t;
	}
	public int getAddress() {return address;}
	public int getOperand() {return operandToBeUpdated;}
	public String getLabelName() {return labelName;}
	public String getType() {return instructionType;}
}@


