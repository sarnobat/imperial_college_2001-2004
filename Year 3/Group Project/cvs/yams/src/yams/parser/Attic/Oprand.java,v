package yams.parser;

/**
 * Oprand of instructions. This class has subclasses that model different types of oprands.
 * 
 * @@author qq01
 */
public abstract class Oprand {
	/**
	 * Instruction needing immediate values as it's oprands.
	 */
	public static final int TYPE_IMMEDIATE = 1;

	/**
	 * Instruction needing register as it's oprands.
	 */
	public static final int TYPE_REGISTER = 0;

	/**
	 * Returns a string representation of the object.
	 * 
	 * @@return a string representation of the object.
	 */
	public String toString() {
		return null;
	}

}
