package exceptions;

public class YAMSException extends Exception {

	/**
	 * 
	 */
	public YAMSException() {
		super();
		// TODO Auto-generated constructor stub
	}

	/**
	 * @@param message
	 */
	public YAMSException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	/**
	 * @@param message
	 * @@param cause
	 */
	public YAMSException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	/**
	 * @@param cause
	 */
	public YAMSException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

}
